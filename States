import React, { useState } from 'react';

function App() {
 const [count, setCount] = useState(0);

 return (
    <div className="App">
      <header className="App-header">
        <h1>Hello React</h1>
        <p>You clicked {count} times</p>
        <button onClick={() => setCount(count + 1)}>
          Click me
        </button>
      </header>
    </div>
 );
}

export default App;
